<main class="main">
  <div class="inner">
      <%# ログインしているときは以下を表示する %>
        <% if user_signed_in? %>
        <div class="greeting">
          <h>こんにちは</h>
          <%= link_to "#{current_user.name}さん", "/users/#{current_user.id}", class: :greeting__link%>

          <%# current_user.nameでログインユーザ名を表示 %>
        </div>
      <% end %>
      <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
       <%= render partial: "prototype", collection: @prototypes  %>
      <%# collectionがeachの代わり　⇒　変数prototypeに値がはいる。 %>

    </div>
  </div>
</main>
